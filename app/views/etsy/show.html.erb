<!doctype html>

<header class="header"> <!-- start header -->
  <div class="containerHead"> <!-- start containerHead -->


    <div> <!-- start logoWrap --> 
      <div class="logo"> <!-- start etsyLogo --> 
        <a href="">
          <img src="assets/etsylogo.20150203215610.png">
        </a>
      </div> <!-- end etsyLogo -->
    </div> <!-- start logoWrap --> 


    <div class="search"> <!-- start searchBar --> 
      <%= form_tag etsy_search_path, method: 'get', 
                                     :items => 'searchForm', 
                                     :class => "searchBox clearfix" do %>
          <%= text_field_tag 'search_query', 
                              params[:search_query], 
                              :class => "searchBar",
                              :placeholder => 'Search for items or shops' %> 
        
          <%= submit_tag 'Search', :class => "squareButton" %> 
        <% end %> 
    </div> <!-- end searchBar -->


    <div class="browseWrap"> <!-- start browseWrap --> 
      <div class="browse clearfix"> <!-- start browse -->
        <a href="">
          <span>
            <span class="spanBrowse">Browse</span>
            <span class="spanDropDown"></span>
        </span>
        </a> 
      </div> <!-- end browse -->
    </div> <!-- end bowseWrap --> 



    <div class="navigation clearfix"> <!-- start navigation -->
        <li> 
          <%= link_to "Cart",      '#' %>
        </li> 
        <li> <%= link_to "You",       '#' %> </li>
        <li> <%= link_to "Favorites", '#' %> </li>
        <li> 
          <div class="icon">
            <div class="home"></div>
            <div class="chimney"></div>
          </div>
          <%= link_to "Home",      '#' %> 
        </li>
    </div> <!-- end navigtion -->

  </div>  <!-- end containerHead -->
</header> <!-- end header -->



<div class="content"> <!-- start content -->
  <div class="containerContent"> <!-- start continerContent -->

    <div class="sideBar"> <!-- start sideBar -->
      <h3>Show results for:</h3>
      <div class="topSideBar"> <!-- start topSideBar -->
        <ul>
          <li><span>testing list</span></li>
          <li>testing list</li>
          <li>testing list</li>
          <li>testing list</li>
          <li>testing list</li>
          <li>testing list</li>
        </ul>
      </div> <!-- end topSideBar -->
      <h3>Refine your search</h3>
      <div class="bottomSideBar"> <!-- start bottomSideBar -->
        <ul>
          <li><span>testing list</span></li>
          <li>testing list</li>
          <li>testing list</li>
          <li>testing list</li>
          <li>testing list</li>
          <li>testing list</li>
        </ul>
      </div> <!-- end bottomSideBar -->
    </div> <!-- end sideBar -->

    <h4>All Categories  “whiskey” (20,497 Results)</h4>
    <div class="itemWrap"> <!-- start itemWrap -->
      <% if @results %>
        <% @results.each do |item| %>
          <div class="item"> <!-- start item -->
            <div class="picture"> <!-- start picture -->
              <a href= <%= item['url'] %> >
                <img src= <%= item['Images'][0]['url_570xN'] %> alt= <%= item['title'] %> >
              </a>
            </div> <!-- end picture -->
              <table> <!-- start table -->
                <thead>
                  <tr>
                    <th class="tableHead clearfix"> 
                      <a href= <%=item['url'] %> > <%= item['title'] %> </a>
                    </th>
                  </tr>
                </thead>
                <tbody class="clearfix">
                  <tr class="tableBody clearfix">
                    <td><a href= <%= item['Shop']['url'] %> > <%= item['Shop']['shop_name'] %></a></td>
                    <td class="clearfix">
                      <span> $ </span>
                      <span> <%= item['price'] %> "&nbsp;"</span>
                      <span> <%= item['Shop']['currency_code'] %> "&nbsp;"</span>
                    </td>
                  </tr>
                </tbody>
              </table> <!-- end table -->
            </div> <!-- end item -->
        <% end %>
      <% end %>
    </div> <!-- end itemWrap -->
  </div> <!-- end containerContent -->
</div> <!-- end content -->


